body{
	margin:0px;
	padding:0px;
}
@media(min-width:1024px) and (max-width:1920px) {
	.log{
	width:100%;
	height:160px;
	margin-top:10px;
	margin-bottom:10px;
}
.log div{
	width:195px;
	height:250px;
	background-image:url(../images/kpimg.png);
	background-repeat:no-repeat;
	background-size:cover;
}
.font01{
	font-family:Arial, Helvetica, sans-serif;
	font-size:18px;
	color:#292828;
}
.logbg{
	
	width:100%;
	height:598px;
	margin-top:10px;
	margin-bottom:10px;
}
.logbg div{
	width:583px;
	height:598px;
	background-image:url(../images/bg3_03.png);
	background-repeat:no-repeat;
	background-size:cover;
}
}
