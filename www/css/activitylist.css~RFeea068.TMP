body{
	margin:0px;
	padding:0px;
}
@media(min-width:1024px) and (max-width:1920px) {
.act{
	width:100%;
	height:68px;
	background-color:#f0f0f0;
}

.d1{
	float:left;
	width:10%;
	height:68px;
}
.d1 .d11{
	width:100%;
	height:143px;
	background:url(../images/bg_18.png);
	background-repeat:no-repeat;
	background-size:46%;
}
.d2{
	float:left;
	width:80%;
	height:68px;
}
.d3{
	float:right;
	width:10%;
	height:68px;
}
.d3 .d33{
	width:100%;
	height:143px;
	margin-top:-38px;
	background:url(../images/bg_05.png);
	background-repeat:no-repeat;
	background-size:46%;
	background-position:right;
}
.buto{
	border:none;
	width:42%;
	height:56px;
	margin-top:6px;
	background:url(../images/bg03_19.png);
	background-repeat:no-repeat;
	background-size:100%;
	background-position:center;
}
.buto2{
	border:none;
	width:42%;
	height:133px;
	margin-left:78px;
	margin-top:6px;
	background:url(../images/bg03_19.png);
	background-repeat:no-repeat;
	background-size:100%;
	background-position:center;
}
.font05{
	color:#FFF;
	font-family:Arial, Helvetica, sans-serif;
	font-size:10px;
}
.bg18{
	width:62px !important;
	height:29px !important;
	margin-left:12px;
	background-size:20px !important;
	margin-top:4px;
	background-position:center center;
	background:url(../images/mdown_17.png);
	display:inline-block;
	background-repeat:no-repeat;
}
.bg19{
	width:62px !important;
	height:29px !important;
	margin-left:8px;
	background-size:29px !important;
	margin-top:4px;
	background-position:center center;
	background:url(../images/globe_28.png);
	display:inline-block;
	background-repeat:no-repeat;
}
.bg20{
	width:62px !important;
	height:29px !important;
	margin-left:10px;
	background-size:29px !important;
	margin-top:2px;
	background-position:center center;
	background:url(../images/act_34.png);
	display:inline-block;
	background-repeat:no-repeat;
}
.bg21{
	width:62px !important;
	height:29px !important;
	margin-left:15px;
	background-size:25px !important;
	margin-top:2px;
	background-position:center center;
	background:url(../images/play_38.png);
	display:inline-block;
	background-repeat:no-repeat;
}
.font06{
	margin-top:5px;
	font-family:Arial, Helvetica, sans-serif;
	font-size:16px;
	color:#000;
}
.font07{
	margin-top:5px;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px;
	color:#868686;
}
}
